import java.util.*;

public class Rotate90Clockwise {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int[][] a = new int[n][n];

        for(int i=0;i<n;i++)
            for(int j=0;j<n;j++)
                a[i][j] = sc.nextInt();
        for(int i=0;i<n;i++) {
            for(int j=i;j<n;j++) {
                int temp = a[i][j];
                a[i][j] = a[j][i];
                a[j][i] = temp;
            }
        }

        for(int i=0;i<n;i++) {
            for(int j=0;j<n/2;j++) {
                int temp = a[i][j];
                a[i][j] = a[i][n-1-j];
                a[i][n-1-j] = temp;
            }
        }
        for(int i=0;i<n;i++) {
            for(int j=0;j<n;j++)
                System.out.print(a[i][j] + " ");
            System.out.println();
        }
    }
}
